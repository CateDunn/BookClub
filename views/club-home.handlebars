<div class="container clubContainer">

   <div class="row  dualHeader">
      <div class="col m6 s12 topPicksLeft">
        <img src="https://dummyimage.com/400x600/d6d2d6/ffffff&text=book+cover" height="500" class="bigbookCover">
      </div>

      <div class="col m6 s12 centeredMobile">
        <h1>{{club.clubName}}</h1>
        <h6 id="creator">Created by: {{club.clubCreator}}</h6>
        <h6 id="description">{{club.description}}</h6>
        <br>
        <h6 id="currentlyReading" class="textstyle2" >Currently Reading:</h6>
        <h4 id="title">{{club.currentlyReading}}</h4>
        <button id="changeBook" class="btn waves-effect waves-light" type="submit">Change Book</button>
        </div> 
   </div>

  <div class="row clubCard bookSearch centeredMobile" id="add">
    <h4>Find a Book for your Club!</h4>
    <form class="create-book">
      <div class="form-group">
        <label for="bookName">Book Title:</label>
        <input type="text" id="bookName" name="name">
      </div>
    </form>
    <button id="addBook" class="btn waves-effect waves-light" type="submit" id="submit">Find Book</button>
  
    <div id="searched-books" ></div>

    <div> 
      <button id="addToClub"  data-id={{club.id}} class="btn waves-effect waves-light {{club.id}}" type="submit">Select this Book</button>
    </div>
  </div>
    
</div>


