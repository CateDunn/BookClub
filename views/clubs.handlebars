<h1>Clubs</h1>

<ul>
  {{#each clubs }}
  <li>Name: {{clubName}}</li>
  <button class="delete-club" data-id="{{this.id}}">DELETE!</button>
  {{/each}}
</ul>

<form class="create-club">
  <div class="form-group">
    <label for="ca">Club Name:</label>
    <input type="text" id="ca" name="name">
  </div>
  <button class="rounded" type="submit" id="submit">Add Club</button>
</form>
